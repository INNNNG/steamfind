[ProcessMgr]
;操作子流程个数
Argc=4

[ProcessMgr_0]
;操作项个数
Argc=2
;是否需要多线程处理
Argument_0=Assoc
; 操作项名称
IsPreLoad_0=0

Argument_1=PreLoad
IsPreLoad_1=0

[ProcessMgr_1]

Argc=2
Argument_0=ImportReg
IsPreLoad_0=

Argument_1=Copy
IsPreLoad_1=0

[ProcessMgr_2]

Argc=1
Argument_0=Main
IsPreLoad_0=0


[Control]
;组件安装状态
InstallStatus=
;白名单
Exclude=Copy,Main

[Component]
;组件安装
[AssocGamePathSet]
;游戏平台关联游戏路径设置
IsNeedAssocGame=1
;关联游戏路径修改,0不关联，1关联
[GameComponent]
;游戏组件dll相对路径
ComponentPath=
;游戏组件dll的文件名称
ComponentName=
;关联游戏ID
AssocGameId=14865

[WindowsCfg]
;该配置项用来匹配游戏运行支持的系统和当前系统
;0默认不检测都支持，1支持PRE_2000，2支持2000，3支持XP,4支持Server2003，5支持Vista，6支持2008，
;7支持win732位，
;8支持win764位
;支持配置多个，以‘，’分隔，例如：1，2；1，3
OSCfg=

[OpenUrl]
Num=2
;使用默认浏览器打开指定的url
Url_0=
Url_1=

[Main]
;是否执行特殊程序
IsSpecial=
SpecialGameExe=
SpecialParam=
;64位系统下是否调用64位程序
IsRun64GameExe=
;要运行的64位程序，相对路径GameExe64=
;运行参数
Param64=
;要运行的程序，相对路径
GameExe=GameSetMan.exe
;运行参数
Param=
;运行方式，默认为0, 1为ShellExecute, 2为WinExec
RunOption=0
;是否展现界面
IsShow=0
;运行后是否关闭程序。用于调试
IsClose=0
;是否上报数据,1是,0否
IsReport=0
;是否使用黑名单机制
IsEnble=0
;黑名单配置路径
EnblePath=
IsInitComponent=0

;文件拷贝配置
[Copy]
;要拷贝的文件数
Num=1

;每个要拷贝的文件，必须以如下格式编写
;[Copy+序列号]
;是否是文件，如果是文件，则为1
;IsFile = 0
;原文件路径，相对路径
;SrcSrc=\C05\WinSxS\Manifests
;目标路径，可以是绝对路径，还可以是通配符的相对路径
;具体通配对照表，请参考文档《ZUJIAN通配符对照表》
;Dst=c:\windows\WinSxS\Manifests

[Copy0]
IsFile=0
Src=YiYi
Dst=C:\Users\Administrator\AppData\Roaming\YiYi
IsCloseRedirection=

[Copy1]
Src=
Dst=
IsCloseRedirection=

;重定向配置项
;该重定向方式只针对文件夹。

[Junction]
;重定向子节点数目
Num=

;每个子节点格式如下
;[Junction+序列号]
;源文件夹，相对路径格式
;Src=NMCopyFile\AppData\Tencent
;目标文件夹，可以用通配符，也可以写死绝对路径
;Dst=C:\Documents and Settings\Hydra\Application Data\Tencent

[junction0]
option=4
src=
dst=

[junction1]
option=4
src=
dst=

[junction2]
src=
dst=

[junction3]
src=
dst=

[JunctionOption]
;重定向节点数目
Num=

;每个子节点格式如下
;[JunctionOption+序列号]
;操作类型0拷贝，5解压缩
;option=5
;执行文件路径，相对路径
;RunName=
;运行参数
;RunParam=
;1是MSI,0是EXE
msi=
;0不等待，1等待
wait=
;例如下面配置项
[JunctionOption0]
option=4
Src=
Dst=

[JunctionOption1]
option=
RunName=
RunParam=
msi=0
wait=1

